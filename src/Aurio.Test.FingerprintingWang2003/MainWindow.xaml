<Window x:Class="Aurio.Test.FingerprintingWang2003.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:my="clr-namespace:Aurio.WaveControls;assembly=Aurio.WaveControls"
        Title="Wang2003 Fingerprinting" Height="462" Width="525" Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Margin="10">
            <Button Name="button1" Content="Process" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click"/>
            <ProgressBar Name="progressBar1" Height="20" Margin="90,0,0,0" VerticalAlignment="Top"/>
        </Grid>
        <my:Spectrogram Grid.Row="1" Name="spectrogram1" Background="Black" Minimum="-90" Margin="10" />
        <my:Spectrogram Grid.Row="2" Name="spectrogram2" Background="Black" Minimum="-90" Margin="10" />
        
    </Grid>
</Window>
