<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0-windows</TargetFramework>
    <AssemblyTitle>Aurio FFmpeg Interface</AssemblyTitle>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\Aurio.licenseheader" Link="Aurio.licenseheader" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="..\Aurio.FFmpeg.Proxy\bin\$(Configuration)\Aurio.FFmpeg.Proxy.dll">
      <Link>ffmpeg\Aurio.FFmpeg.Proxy.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="..\Aurio.FFmpeg.Proxy\bin\$(Configuration)\avcodec-57.dll">
      <Link>ffmpeg\avcodec-57.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="..\Aurio.FFmpeg.Proxy\bin\$(Configuration)\avformat-57.dll">
      <Link>ffmpeg\avformat-57.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="..\Aurio.FFmpeg.Proxy\bin\$(Configuration)\avutil-55.dll">
      <Link>ffmpeg\avutil-55.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="..\Aurio.FFmpeg.Proxy\bin\$(Configuration)\swresample-2.dll">
      <Link>ffmpeg\swresample-2.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="..\Aurio.FFmpeg.Proxy\bin\$(Configuration)\swscale-4.dll">
      <Link>ffmpeg\swscale-4.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Aurio\Aurio.csproj" />
  </ItemGroup>
</Project>