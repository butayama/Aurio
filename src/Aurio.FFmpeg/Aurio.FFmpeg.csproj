<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0-windows</TargetFramework>
    <AssemblyTitle>Aurio FFmpeg Interface</AssemblyTitle>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\Aurio.licenseheader" Link="Aurio.licenseheader" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="..\Aurio.FFmpeg.Proxy\bin\$(Configuration)\Aurio.FFmpeg.Proxy.dll">
      <Link>ffmpeg\Aurio.FFmpeg.Proxy.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="..\Aurio.FFmpeg.Proxy\bin\$(Configuration)\avcodec-60.dll">
      <Link>ffmpeg\avcodec-60.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="..\Aurio.FFmpeg.Proxy\bin\$(Configuration)\avformat-60.dll">
      <Link>ffmpeg\avformat-60.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="..\Aurio.FFmpeg.Proxy\bin\$(Configuration)\avutil-58.dll">
      <Link>ffmpeg\avutil-58.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="..\Aurio.FFmpeg.Proxy\bin\$(Configuration)\swresample-4.dll">
      <Link>ffmpeg\swresample-4.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="..\Aurio.FFmpeg.Proxy\bin\$(Configuration)\swscale-7.dll">
      <Link>ffmpeg\swscale-7.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Aurio\Aurio.csproj" />
  </ItemGroup>
</Project>